<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewBag.Title - LMS.Web</title>

    <meta charset="utf-8" />
    <meta name="renderer" content="webkit">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">

    <!--[if lt IE 9]>
    <meta http-equiv="refresh" content="0;ie.html" />
    <![endif]-->
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <link href="~/lib/layuiadmin/layui/css/layui.css" rel="stylesheet" media="all" />
    <link href="~/lib/layuiadmin/style/admin.css" rel="stylesheet" media="all" />
</head>
<body class="layui-layout-body">

    @RenderBody()

    @RenderSection("Scripts", required: false)


    <script src="~/lib/layuiadmin/layui/layui.js" type="text/javascript"></script>
    <script type="text/javascript">
        layui.config({
            base: '../lib/layuiadmin/' //静态资源所在路径
        }).extend({
            index: 'lib/index' //主入口模块
        }).use('index', 'console');
    </script>
</body>

</html>
